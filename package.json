{
  "name": "markdown-it-custom-block",
  "description": "Custom blocks for markdown-it",
  "version": "1.0.0",
  "packageManager": "pnpm@10.2.1",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "mocha --colors",
    "dev": "npm-watch"
  },
  "watch": {
    "unit": {
      "patterns": [
        "index.js",
        "test/fixtures/*.md",
        "test"
      ],
      "extensions": "js,md,txt"
    }
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown"
  ],
  "engines": {
    "node": ">= 22.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/markdown-it-custom-block.git"
  },
  "bugs": {
    "url": "https://github.com/posva/markdown-it-custom-block/issues"
  },
  "homepage": "https://github.com/posva/markdown-it-custom-block#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "markdown-it": "^14.1.0",
    "markdown-it-testgen": "^0.1.6",
    "mocha": "^11.1.0",
    "npm-watch": "^0.13.0",
    "prettier": "^3.5.0"
  }
}
