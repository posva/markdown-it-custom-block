{
  "name": "markdown-it-custom-block",
  "description": "Custom blocks for markdown-it",
  "version": "0.1.2",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "yarn run unit",
    "unit": "mocha --colors",
    "dev": "npm-watch"
  },
  "watch": {
    "unit": {
      "patterns": [
        "index.js",
        "test/fixtures/*.md",
        "test"
      ],
      "extensions": "js,md,txt"
    }
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown"
  ],
  "engines": {
    "node": ">= v14.17.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/markdown-it-custom-block.git"
  },
  "bugs": {
    "url": "https://github.com/posva/markdown-it-custom-block/issues"
  },
  "homepage": "https://github.com/posva/markdown-it-custom-block#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/markdown-it": "^12.0.3",
    "markdown-it": "^12.0.2",
    "markdown-it-testgen": "^0.1.4",
    "mocha": "^9.0.3",
    "npm-watch": "^0.10.0"
  }
}
